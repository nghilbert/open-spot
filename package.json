{
  "name": "openspot",
  "version": "1.0.0",
  "description": "OpenSpot Description",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "clean": "rimraf ./dist ./node_modules ./frontend/node_modules ./backend/node_modules",
    "dev-frontend": "npm run dev --workspace frontend",
    "dev-backend": "npm run dev --workspace backend",
    "dev": "run-p dev-backend dev-frontend",
    "start": "node ."
  },
  "workspaces": [
    "backend",
    "frontend",
    "shared"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1"
  }
}
