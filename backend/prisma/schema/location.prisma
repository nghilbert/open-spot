enum Type {
  PARKING_LOT
  BIKE_RACK
  NONE
}

model Location {
  id               Int     @id @default(autoincrement())
  addressId        Int     @unique
  address          Address @relation(fields: [addressId], references: [id])
  name             String
  spotCapacity     Int
  spotAvailability Int
  permit           Permit  @default(NONE)
  type             Type    @default(NONE)
  timeLimitSecs    Int?
  timers           Timer[]
  historicalTimers HistoricalTimes[]
  averageParkingTime Int @default(0)
}
