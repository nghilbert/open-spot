<!DOCTYPE html>
<html>
    <head>
        <title>Open Spot - Test Login</title>
    	<link rel="icon" href="/favicon.svg" type="image/svg+xml" style ="width: 50px; height: 50px;">
    </head>
    <body>
        Name: <span id="name">N/A</p>
    </body>
</html>

<script>
    let nameRes = await fetch(`/api/user/name`);
    let name = await nameRes.text();

    let nameForm = document.getElementById("name");
    if(nameForm){
        nameForm.innerHTML = name;
    }
</script>